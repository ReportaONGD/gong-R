<div class="linea" id="<%=params[:update_listado]%>_formulario_anadir" ></div>
<%= listado( [["Presupuestos del empleado", "", "2"]], :url =>  {:action => 'editar_nuevo', :empleado_id => (@empleado ? @empleado.id : nil), :update_listado => params[:update_listado],  :update => params[:update_listado] + "_formulario_anadir"}, :cerrar_listado => true, :id_listado => params[:update_listado] ) %>

<div class="linea" id="<%=params[:update_listado]%>_nuevo">   
</div> 
<% j = 0 %>
<% for presupuesto in @presupuestos %>
  <% fila = params[:update_listado] +"_"+ (j += 1).to_s %>
  <%= render :partial => "presupuesto", :locals => {:update => fila, :presupuesto => presupuesto, :empleado => @empleado} %>
<% end %>
<%= final_listado %>
